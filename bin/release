#!/usr/bin/env bash
# bin/release <build-dir>
cat << EOF
addons:
config_vars:
default_process_types:
   web: JMETER_HOME=/app/local/apache-jmeter-3.2 JAVA_HOME=/app/local/jdk PATH=/app/local/jdk/bin:/app/local/apache-jmeter-3.2/bin:\$PATH jmeter -n -JTEST_URL=$TEST_URL -t /app/local/$LOAD_SCRIPT
EOF
